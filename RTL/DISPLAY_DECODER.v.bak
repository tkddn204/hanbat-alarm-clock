module DISPLAY_DECODER(
	RESETN, CLK,
	IN_TIME, IN_DATE, IN_ALARM_TIME,
	OUT_H10, OUT_H1, OUT_M10, OUT_M1, OUT_S10, OUT_S1,
	OUT_Y10, OUT_Y1, OUT_MT10, OUT_MT1, OUT_D10, OUT_D1,
	ALARM_H10, ALARM_H1, ALARM_M10, ALARM_M1, ALARM_S10, ALARM_S1
);

input RESETN, CLK;
input [16:0] IN_ALARM_TIME;
input [17:0] IN_TIME;
input [15:0] IN_DATE;
output wire [7:0] ALARM_H10, ALARM_H1, ALARM_M10, ALARM_M1, ALARM_S10, ALARM_S1;
output wire [7:0] OUT_ALARM_H10, OUT_ALARM_H1, OUT_ALARM_M10;
output wire [7:0] OUT_ALARM_M1, OUT_ALARM_S10, OUT_ALARM_S1;

// Current
reg [4:0] HOUR;
reg [5:0] MIN, SEC;

reg [6:0] YEAR;
reg [3:0] MONTH;
reg [4:0] DAY;

wire [7:0] H10, H1, M10, M1, S10, S1;
wire [7:0] Y10, Y1, MT10, MT1, D10, D1;

wire [7:0] OUT_H10, OUT_H1, OUT_M10, OUT_M1, OUT_S10, OUT_S1;
wire [7:0] OUT_Y10, OUT_Y1, OUT_MT10, OUT_MT1, OUT_D10, OUT_D1;

// Alarm
reg [4:0] ALARM_HOUR;
reg [5:0] ALARM_MIN, ALARM_SEC;
